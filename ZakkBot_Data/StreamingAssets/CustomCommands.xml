<?xml version="1.0" encoding="utf-8"?>
<!-- Valid response arguments include Target and other -->
<!-- Target will refer to the chatter that called the command -->
<!-- Other will refer to the contents of the chat message after the command -->
<Commands>
  <Command command="social" createButton="true" editable="true">
    <Methods>
      <Method>
        <MethodName>SendMessage</MethodName>
        <Parameters>
          <Parameter type="content">Subscribe to my YouTube: . Follow me on Twitter: .</Parameter>
        </Parameters>
      </Method>
    </Methods>
  </Command>
  <Command command="hi" createButton="false" editable="true">
    <Methods>
      <Method>
        <MethodName>SendMessage</MethodName>
        <Parameters>
          <Parameter type="content">Hi, </Parameter>
          <Parameter type="argument">Target</Parameter>
        </Parameters>
      </Method>
    </Methods>
  </Command>
</Commands>